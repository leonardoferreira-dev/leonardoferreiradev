datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Bid {
  id        Int      @id @default(autoincrement())
  name      String
  contact   String
  amount    Decimal  @db.Decimal(10, 2) // Definindo precis√£o e escala para o campo decimal
  timestamp DateTime
}
